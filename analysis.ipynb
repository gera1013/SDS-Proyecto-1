   "source": [
    "def get_average(x):\n",
    "    values = x.split(\",\")\n",
    "    sum = 0\n",
    "    avg = 0\n",
    "    \n",
    "    if len(values) == 0:\n",
    "        return avg \n",
    "    \n",
    "    else:\n",
    "        for value in values:\n",
    "            if value == \"\":\n",
    "                value = 0\n",
    "            sum += int(value)\n",
    "\n",
    "        avg = sum / len(values)\n",
    "    \n",
    "    return avg"
   ]
  },
